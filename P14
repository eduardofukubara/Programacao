v1 = float(input("Digite o valor 1: "))
v2 = float(input("Digite o valor 2: "))
v3 = float(input("Digite o valor 3: "))

if (v1>v2)and(v1>v3):
  maior=v1
if (v2>v1)and(v2>v3):
  maior=v2
if (v3>v1)and(v3>v2):
  maior=v3
else:
  maior=v3
  print(f"O maior deles Ã©: {maior:.2f} ")

